package com.ly.module1.dao;

import org.springframework.stereotype.Repository;

import com.ly.bean.base.BaseDao;
import com.ly.bean.pojo.Pojo;

@Repository
public class PojoDao extends BaseDao<Pojo>{
	public Pojo getPojoById(String id){
		Pojo pojo = (Pojo)this.getSession().get(Pojo.class, id);
		return pojo;
	}
}
